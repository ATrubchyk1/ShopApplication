{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MailJet": {
    "ApiKey" : "your ApiKey",
    "ApiSecret" : "your ApiSecret"
  },
  "SMTPMail" : {
    "SmtpServer" : "your SmtpServer",
    "Port" : 587,
    "FromEmail" : "your FromEmail",
    "Password" : "your Password"
  },
