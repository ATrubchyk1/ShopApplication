    "Environment" : "sandbox Environment",
    "MerchantId" : "your MerchantId",
    "PublicKey": "your PublicKey",
    "PrivateKey" : "your PrivateKey"
  },
  "ConnectionStrings" : {
    "DefaultConnection" : "Server=localhost;Database=yourDatabase;Uid=root;Pwd=password"
  },
  "Authentication" : {
    "Facebook" : {
      "AppId" : "your AppId",
      "AppSecret" : "your AppSecret"
    }
  }
}
